Algoritmo Ejercicio10
	//Declarar variables
	Definir a, h, area, perimetro como real;
	
	//Entrada de datos
	Escribir "Digite el ancho";
	Leer a;
	Escribir "Digite el alto";
	Leer h;
	
	//Proceso 
	area <- a*h;
	perimetro <- (a*2)+(h*2);
	
	//Salida de datos
	Escribir "El area del rectángulo es: ", area,", el pedimetro es: ",perimetro;
	
FinAlgoritmo
