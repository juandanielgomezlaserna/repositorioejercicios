Algoritmo Ejercicio30
	//DECLARACIÓN DE VARIABLES
	Definir num1, num2 Como Real;
	//ENTRADA DE DATOS
	Escribir "Ingrese el número 1";
	leer num1;
	Escribir "Ingrese el número 2";
	leer num2;
	
	//PROCESO Y SALIDA
	si num2 = 0 Entonces
		Escribir "No se puede dividir entre 0";
	SiNo
		Escribir "La división da: ",num1/num2;
	FinSi
	
FinAlgoritmo
